{
  "expo": {
    "name": "pintuClone",
    "slug": "pintuClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pintu.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/pintu.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "BASE_URL": "https://api.pintu.co.id/v2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pintu.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/pintu.png"
    }
  }
}
