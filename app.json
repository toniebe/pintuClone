{
  "expo": {
    "name": "pintuClone",
    "slug": "pintuClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pintu.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/pintu.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "extra": {
      "BASE_URL": "https://api.pintu.co.id/v2",
      "eas": {
        "projectId": "bcc2e0cf-1070-4ed7-a45d-98ecb7627b5b"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pintuClone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pintu.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.pintuClone"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/pintu.png"
    }
  }
}
